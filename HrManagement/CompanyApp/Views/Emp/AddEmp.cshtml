<form action="/emp/addEmp" method="post">

    int empId, string firstName, string lastName, string email, string password, double sal

    <div class="form-group">
        <label for="id">Id:</label>
        <input type="number" required class="form-control" id="id" name="empId">
    </div>
    <div class="form-group">
        <label for="name">First Name:</label>
        <input type="text" required class="form-control" id="name" name="firstName">
    </div>

    <div class="form-group">
        <label for="address">Last Name:</label>
        <input type="text" required class="form-control" id="address" name="lastName">
    </div>
    <div class="form-group">
        <label for="email">Email address:</label>
        <input type="email" required class="form-control" id="email" name="email">
    </div>
    <div class="form-group">
        <label for="pwd">Password:</label>
        <input type="password" required class="form-control" id="pwd" name="password">
    </div>
    <div class="form-group">
        <label for="id">Salary:</label>
        <input type="number" required class="form-control" id="id" name="sal">
    </div>

    <button type="submit" class="btn btn-default btn-success">Register</button>
</form>